def convert(one_string):
    new_string = one_string.replace('-', '')
    new_string = one_string[0].lower() + new_string[1:]
    return new_string
}%

import UIKit

// swiftlint:disable file_length

extension UIFont {

    // swiftlint:disable type_body_length

    public enum SystemName: String, CaseIterable {
% for pair in uifont_familynames_fontnames:

        // MARK: ${pair[0]}

%   for fontname in pair[1]:
        case ${convert(fontname)} = "${fontname}"
%   end
% end
    }

    // swiftlint:enable type_body_length
}

extension UIFont {

    public convenience init?(systemName: UIFont.SystemName, size: CGFloat) {
        self.init(name: systemName.rawValue, size: size)
    }
}
